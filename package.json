{
  "author": "Anephenix <paul@anephenix.com> (http://anephenix.com)",
  "name": "satellite",
  "description": "Extensions for node-http-proxy",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "http://github.com/paulbjensen/satellite.git"
  },
  "dependencies" : {
    "http-proxy" : "1.1.4"
  },
  "devDependencies" : {
    "coffee-script" : "1.7.1",
    "docco"         : "0.6.3",
    "connect"       : "2.15.0",
    "mocha"         : "1.18.2"
  },
  "optionalDependencies" : {
    "redis" : "0.10.1"
  },
  "main"    : "./lib/satellite",
  "engines" : {
    "node"  : "0.10.X"
  },
  "scripts" : {
    "build" : "node_modules/.bin/cake build",
    "docs"  : "node_modules/.bin/cake docs",
    "test"  : "NODE_ENV=test node_modules/.bin/mocha --compilers coffee:coffee-script/register test/satellite_test.coffee test/**/*"
  }
}